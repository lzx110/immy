  <sqlMap resource="com/immy/model/SYSTEM.xml"/>
  
  <sqlMap resource="com/immy/mapper/product_SqlMap.xml.xml"/>
  <sqlMap resource="com/immy/mapper/product_type_SqlMap.xml"/>
  <sqlMap resource="com/immy/mapper/user_SqlMap.xml"/>
  <sqlMap resource="com/immy/mapper/user_product_SqlMap.xml"/>
  </sqlMapConfig>    
